.row{
    background-color: #fff;
    padding-left: 30rpx;
    margin: 30rpx 0;
}
.row>view{
    padding: 30rpx 0;
}
.row_t{
    font-size: 28rpx;
}
.row_status{
    float: right;
    color: #ffda44;
    margin-right: 30rpx;
}
.row_name{
    font-size: 36rpx;
    margin-bottom: 20rpx;
}
.row_content{
    font-size: 28rpx;
    color: #999;
    margin-bottom: 6rpx;
}
.row_b{
    position: relative;
}
.row_b text{
    font-size: 28rpx;
}
.row_b_btn{
    position: absolute;
    right: 30rpx;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    padding: 20rpx 40rpx;
    border: 1rpx solid #e4e4e4;
    border-radius: 8rpx;
}
.row_b_r{
    position: absolute;
    right: 30rpx;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}
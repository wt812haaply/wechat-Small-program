page{
    color: #333;
    background-color: #efeff4;
}
view,text,textarea{
    box-sizing: border-box;
}
.clearfix::after{
    clear: both;
    content: "";
    display: block;
    height: 0px;
    visibility: hidden;
}
.border_b{
    border-bottom: 1rpx solid #e4e4e4;
}
.contact{
    position: fixed;
    right: 15rpx;
    bottom: 10%;
    width: 100rpx;
    height: 100rpx;
    font-size: 24rpx;
    border-radius: 100%;
    background-color: #ccc;
    text-align: center;
}
.contact image{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}
.contact contact-button{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}
.w100{
    width: 100%;
}
.h100{
    height: 100%
}

swiper{
    height: 450rpx;
}
.slide-image{
    width: 100%;
    height: 450rpx;
}


.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{
     margin-bottom: 2rpx;
}
.swiper-box .wx-swiper-dot{
    width:40rpx;
    display: inline-flex;
    height: 10rpx;
    margin-left: 20rpx;
    justify-content:space-between;
}
.swiper-box .wx-swiper-dot::before{
    content: '';
    flex-grow: 1; 
    background: rgba(255,255,255,0.8);
    border-radius: 8rpx
}
.swiper-box .wx-swiper-dot-active::before{
    background:rgba(244,0,0,0.8);   
}



/*1*/
.one{
    width: 100%;
}
.one-title{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 90rpx;
    background: #FC6E74;
    color: #fff;
    padding: 0 24rpx;
    margin:10px 0;
}
.one-title-L,.one-title-R{
    font-size: 14px;   
}
.one-con{
    width: 100%;
}
.one-list{
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}
.one-list-con{
    width: 46%;
    /*height: 100px;*/
    margin: 0 2% 2% 2%;
    background: red;
    display: flex; 
    flex-direction: column;  
     position: relative;
}
.one-list-con-img{
    display: flex;
    height: 208rpx;
}
.one-list-con-img image{
    width:100%;
    height: 208rpx;
}
.one-list-con-name,.one-list-con-tag{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    color: #fff;
}
.one-list-con-name{
    font-size: 12px;
    height:30px;
    background: #F8E367;
}

.one-list-con-tag{
    position: absolute;
    top:0px;
    left: 0px;
    font-size: 12px;
    background: rgba(220,100,35,.5);    
    width: 40px;
    height: 40px;
}

/*2*/
.two{
    width: 100%;
}
.two-title{
    display: flex;
    align-items: center;
    justify-content: center;
    height: 45px;
    width: 100%;
    margin: 10px 0;
    background: #53FF41;
}
.two-con{
    
}

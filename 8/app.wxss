.art,
.arb,
.arr {
    content: "";
    width: 8px;
    height: 8px;
    border: 1px solid #b2b2b2;
    border-width: 0 0 1px 1px;
    display: inline-block;
    vertical-align: middle;
}

.art {
    transform: rotate(135deg);
    -webkit-transform: rotate(135deg);
}

.arb {
    transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
}

.arr {
    transform: rotate(-135deg);
    -webkit-transform: rotate(-135deg);
}


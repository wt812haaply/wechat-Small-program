/**
    global style
*/

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.container button {
    margin: 20px;
    width: 200px;
}

body {
    overflow: hidden;
}

p,
ul,
li,
dl,
dd,
dt,
scroll-view {
    display: block;
    box-sizing: border-box;
}

i {
    font-style: normal;
}

b {
    font-weight: 700;
}

image {
    border: 0;
}

view,
ul,
li,
dl,
dd,
dt,
p,
input {
    color: #333;
    font-size: 28rpx;
    font-family: "Helvetica Neue", Helvetica, "Microsoft YaHei", STHeiTi, sans-serif;
    line-height: 42rpx
}

audio,
canvas,
progress,
video {
    display: inline-block;
    vertical-align: baseline
}

ul,
li,
dl,
dd {
    padding: 0;
    margin: 0;
    list-style-type: none;
}

view,
ul,
li,
i,
p,
dl,
dd,
dt,
button {
    box-sizing: border-box;
}

